<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<Authors>p3root</Authors>
		<Company>p3software</Company>
		<Product>Automatica.Core</Product>
		<PackageProjectUrl>https://github.com/automatica-core</PackageProjectUrl>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Description>Base interfaces for automatica.core system</Description>
		<Copyright>p3root and automatica.core</Copyright>
		<RepositoryUrl>https://github.com/automatica-core</RepositoryUrl>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.1" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Automatica.Core.EF\Automatica.Core.EF.csproj" />
	</ItemGroup>

</Project>
